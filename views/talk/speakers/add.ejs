<div class="sch-b-content--default">
  <h1>Add Speaker to talk</h1>
  <form class="sch-b_form" method="post">
    <input type='hidden' name='_csrf' value='<%= _csrf %>'>
    <div class="form-field">
      <label for="userid">Speaker ID</label>
      <input type="text" id="userid" name="userid" class="sch-e_input-text">
    </div>

    <div class="form-actions">
      <input type="submit" class="sch-e_green-button" value="Add speaker">
    </div>
  </form>
</div>
